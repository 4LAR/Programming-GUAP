 <paragraph index="490" node_type="writer" parent_index="389">          &lt;td&gt;Типы данных&lt;/td&gt;</paragraph>
 <paragraph index="491" node_type="writer" parent_index="389">          &lt;td&gt;Безопасность типов (null safety)&lt;/td&gt;</paragraph>
 <paragraph index="492" node_type="writer" parent_index="389">          &lt;td&gt;Стандартные типы данных&lt;/td&gt;</paragraph>
 <paragraph index="493" node_type="writer" parent_index="389">        &lt;/tr&gt;</paragraph>
 <paragraph index="494" node_type="writer" parent_index="389">        &lt;tr&gt;</paragraph>
 <paragraph index="495" node_type="writer" parent_index="389">          &lt;td&gt;Синтаксис&lt;/td&gt;</paragraph>
